    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.53",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "globals": "^16.5.0",
    "oxlint": "^1.33.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "prettier": {
